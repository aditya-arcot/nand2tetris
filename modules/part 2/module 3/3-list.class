/** represents linked list of integers */
class List {
	field int data;
	field List next;

	/* creates a List */
	constructor List new(int car, List cdr) {
		let data = car;
		let next = cdr;
		return this;
	}

	/** prints this List */
	method void print() {
		var List current;
		let current = this;

		while (~(current == null)) {
			do Output.printInt(current.getData());
			do Output.printChar(32); // print space
			do current = current.getNext();
		}
		
		return;
	}

	/** disposes this List */
	// recursively disposes tail
	method void dispose() {
		if (~(next == null)) {
			do next.dispose();
		}
		//uses OS routine to recycle
		do Memory.deAlloc(this);
		return;
	}

}
